{
    "name": "Transformer",
    "ntoken": 5,
    "train_start": "2016-01-01",
    "valid_start": "2023-12-31",
    "test_start": "2024-06-01",
    "end_date": "2024-12-31",
    
    "epochs": 50,
    "batch_size": 8,
    "val_type": "asset",
    "optimizer": {
        "type": "Adam",
        "args":{
            "lr": 0.001,
            "weight_decay": 0.00001,
            "amsgrad": true
        }
    },
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 1,
            "gamma": 0.5
        }
    },    

    "d_model": 6,
    "dropout": 0.1, 
    "d_hid": 128, 
    "nhead": 2, 
    "nlayers_e": 6, 
    "nlayers_d": 6
